   "source": [
    "1. Intersection "
   ]
  },
  {
   "cell_type": "raw",
   "id": "a9f95d8e-b513-4c06-af93-374c2496ac62",
   "metadata": {},
   "source": [
    "A = {1,2,3,4,5,6,7,8}\n",
    "B = {5,6,7,8}\n",
    "\n",
    "A ∩ B = {5,6,7,8}  #common in both"
   ]
  },
  {
   "cell_type": "code",
